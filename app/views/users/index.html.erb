<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>User id</th>
      <th>Password</th>
      <th>User type</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.user_id, user %></td>
        <td><%= user.password %></td>
        <td><%= user.user_type %></td>
      </tr>
    <% end %>
  </tbody>
</table>
